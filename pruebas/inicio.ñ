// Biblioteca estándar

// Aleatorio
externo ent aleatorio();
externo ent saleatorio(ent semilla);
// ES
externo ent escribe(nat8* _texto);
externo ent escribeln(nat8* _texto);
// Proceso
externo ent idprocesoactual();

// Módulos incorporados
externo nada prueba_suma();
externo nada prueba_resta();
externo nada prueba_multiplicación();
externo nada prueba_división();
externo nada prueba_resto();
externo nada prueba_vector();
externo nada prueba_función();
externo nada prueba_si_condicional();
externo nada prueba_bucle_mientras();
externo nada prueba_comentarios();

público ent inicia()
{
    prueba_suma();
    prueba_resta();
    prueba_multiplicación();
    prueba_división();
    prueba_resto();
    prueba_vector();
    prueba_función();
    prueba_si_condicional();
    prueba_bucle_mientras();
    prueba_comentarios();

    devuelve 0;
}