público ent inicia()
{
    nat16[] vector = [288, 42];

    ent número = vector[1] / 42;

    // Caracteres UTF-8 de "¡Hola mundo!"
    nat8[] caracteres = [194, 161, 72, 111, 108, 97, 32, 109, 117, 110, 100, 111, 33];

    ent*[1] puntero; // = número;

    holamundo();

    si (número < 2)
    {
        ent y = duplica(número);
        número = y;
        devuelve número;
    }
    sino
    {
        //número = prueba(vector);
        devuelve número;
    }

    devuelve número;
}