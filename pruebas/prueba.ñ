externo ent64 escribe(nat8* _texto);
externo ent64 escribeln(nat8* _texto);

público ent64 inicia()
{
    //nat8[] texto = "¡Hola, mundo!";

    nat8[] correcto = ".";
    nat8[] error = "\nERROR\n";

    escribe(correcto);
    escribe(correcto);
    escribe(correcto);
    escribe(correcto);
    escribe(error);
    escribe(correcto);
    escribe(correcto);

    devuelve 42;
}