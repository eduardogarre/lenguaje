externo ent holamundo();
externo ent duplica(ent x);

público ent inicia()
{
    nat16[] vector = [288, 42];

    ent número = vector[1] / 1;

    // Caracteres UTF-8 de "¡Hola mundo!\0"
    nat8[] caracteres = [194, 161, 72, 111, 108, 97, 32, 109, 117, 110, 100, 111, 33, 0];

    holamundo();

    ent* pruebaPuntero = &número;
    *pruebaPuntero = *pruebaPuntero + 1 + 42;

    //nat8* texto = caracteres;

    si (número < 2)
    {
        ent y = duplica(número);
        número = y;
        devuelve número + 2;
    }
    sino
    {
        //número = prueba(vector);
        devuelve número;
    }

    devuelve número;
}