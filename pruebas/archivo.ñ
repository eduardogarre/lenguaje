externo ent duplica(ent x);
externo ent escribeln(nat8* _texto);

público ent inicia()
{
    nat16[] vector = [288, 42];

    ent número = vector[1] / 42;

    nat8[] hola = "¡Hola, mundo!";
    nat8[] adiós = "¡Adiós, mundo!";

    si (número < 2)
    {
        escribeln(hola);
        devuelve duplica(número);
    }
    sino
    {
        escribeln(adiós);
        devuelve número;
    }

    devuelve número;
}