externo ent hola();
externo ent mundo();
externo ent holamundo();
externo ent duplica(ent x);

público ent inicia()
{
    nat16[] vector = [288, 42];

    ent número = vector[0] / 2;

    //ent número = 288 / 2;

    holamundo();

    si (número < 2)
    {
        //ent y = duplica(42);
        ent y = duplica(número);
        número = y;
    }
    sino
    {
        número = número;
    }

    holamundo();

    devuelve número;
}