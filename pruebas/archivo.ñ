// Biblioteca estándar

// Aleatorio
externo ent aleatorio();
externo ent saleatorio(ent semilla);
// ES
externo ent escribe(nat8* _texto);
externo ent escribeln(nat8* _texto);
// Proceso
externo ent idprocesoactual();

// Módulos de Ñ
externo ent duplica(ent x);

público ent inicia()
{
    //nat16[] vector = [288, 42];

    //ent número = vector[1] / 42;

    // Prepara aleatorio
    ent64 idp = idprocesoactual();
    saleatorio(idp);

    // Obtengo número aleatorio
    ent número = aleatorio();

    bool paridad = (número % 2) == 0;

    nat8[] par = "¡Par!";
    nat8[] impar = "¡Impar!";

    si (paridad)
    {
        escribeln(par);
        devuelve número;
    }
    sino
    {
        escribeln(impar);
        devuelve número;
    }

    devuelve número;
}