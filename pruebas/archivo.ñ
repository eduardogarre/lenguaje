público ent inicia()
{
    nat16[] vector = [288, 42];

    ent número = vector[1] / 42;

    //ent número = 288 / 2;

    holamundo();

    si (número < 2)
    {
        ent y = duplica(número);
        número = y;
        devuelve número;
    }
    sino
    {
        //número = prueba(vector);
        devuelve número;
    }

    devuelve número;
}